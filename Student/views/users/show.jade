extends ../layout

block content
  #{record.name}
  .container
    center
      h1 Information of Students
      table.table.table-striped
        tr
          th Student Name
          th Course
          th Fee
          th Date of admission
          th Contact No.
        tr
          td #{record.name}
          td #{record.course}
          td #{record.fee} 
          td #{record.doa} 
          td #{record.contact} 